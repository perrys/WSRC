#!/usr/bin/env python

import getopt
import sys

if __name__ == "__main__":
  if len(sys.argv) < 2:
    sys.stderr.write("USAGE: wsrc <command> [arguments]\n")
    sys.exit(1)

  command = sys.argv[1]
  if command in ("sync"):
    import wsrc.booking.main
    wsrc.booking.main.run_from_command_line()

# Local Variables:
# mode: python
# End:
